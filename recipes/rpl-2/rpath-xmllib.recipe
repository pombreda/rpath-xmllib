class rPathXmlLib(PackageRecipe):
    name = 'rpath-xmllib'
    version = ''

    buildRequires = [
        'lxml:python',
        'make:runtime',
        'python:devel',
        'util-linux:runtime'
    ]

    def setup(r):
        r.addMercurialSnapshot('http://hg.rpath.com/rpath-xmllib')
        r.MakeInstall()
